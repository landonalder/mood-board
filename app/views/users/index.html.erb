<% @users.each do |user| %>
  <%= link_to(
        image_tag(
          user.image_url,
          alt: user.full_name,
          title: user.full_name,
          id: user.slug,
          class: 'img-small img-rounded'
        ),
        user_path(user)
      )%>
<% end %>
<br><br>
<%= link_to("Sign Out", destroy_user_session_path, method: :delete) %>